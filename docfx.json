{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "**/toc.yml",
          "**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "index.html"
          ,"**/images/**"
          ,"**.jpg"
          ,"**.jpeg"
          ,"**.png"
          ,"**.bmp"
          ,"**.ico"
          ,"**.gif"
          ,"**.mov"
          ,"**.mkv"
          ,"**.mp4"
          ,"**.pptx"
          ,"**.css"
          ,"**.js"
          ,"**.ttf"
          ,"**.woff"
          ,"**.svg"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",  "templates/altazion"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "sitemap":{
      "baseUrl": "https://aide.altazion.com/",
      "priority": 0.3,
      "changefreq": "weekly",
      "fileOptions":{
          "**.yml": {
              "priority": 0.5
          }
      }
    }
  }
}