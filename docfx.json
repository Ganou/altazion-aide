{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "**.yml",
          "**.md"
        ],
        "exclude": [
          "azure-pipelines.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "index.html"
          ,"**/images/**"
          ,"**.jpg"
          ,"**.jpeg"
          ,"**.png"
          ,"**.bmp"
          ,"**.ico"
          ,"**.gif"
          ,"**.mov"
          ,"**.mkv"
          ,"**.mp4"
          ,"**.pptx"
          ,"**.xls"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}